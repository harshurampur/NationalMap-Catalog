{
  "filterQuery": [
    {
        fq: '(organization:<%= org %> AND res_format:(geojson OR GeoJSON OR kml OR KML OR kmz OR KMZ OR wms OR WMS OR CSV-GEO-AU OR csv-geo-au OR aus-geo-csv OR "Esri REST"))'
    }
  ],
  "url": "http://localdata.net.au",
  "name": "<%= name %>",
  "type": "ckan",
  "includeCsv": true,
  "includeGeoJson": true,
  "includeKml": true,
  "includeWms": true,
  "includeWfs": true,
  "includeEsriMapServer": true,
  "ungroupedTitle": ""
}
